<table>
  <thead>
  <tr>
    <th>Name</th>
    <th>Category</th>
  </tr>
  </thead>
  <tbody>
  {{#each oaccount in model}}
    <tr>
      <td>
        {{#link-to 'oaccount' oaccount}}
          {{oaccount.name}}
        {{/link-to}}
      </td>
      <td>
        {{oaccount.category.name}}
      </td>
    </tr>
  {{else}}
    <tr>
      <td colspan="2">No other accounts found.</td>
    </tr>
  {{/each}}
  </tbody>
</table>
